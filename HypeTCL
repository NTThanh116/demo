@echo off

REM ===== Loop Contour =====
for %%C in (11 12 13 14 15 16 21 22 23 24 25 26 31 32 33 34 35 36) do (
    mkdir Contour_%%C
    cd Contour_%%C

    REM ===== Loop Subcase =====
    for %%S in (10 11 12 13 14) do (
        mkdir Subcase_%%S
        mkdir Subcase_%%S\287
    )

    cd ..
)

echo Done!
pause
